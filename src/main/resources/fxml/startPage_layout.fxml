<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane fx:id="startPane" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1"
           style="-fx-background-image: url('/background/mainPageBackground.png'); -fx-background-size: cover;"
           stylesheets="@/css/MainAppStyle.css">
   <children>
      <VBox fx:id="vertContainer" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Button fx:id="startButton" mnemonicParsing="false" text="Start" alignment="CENTER" />
            <Button fx:id="howTo" mnemonicParsing="false" text="How to Play" alignment="CENTER"/>
         </children>
      </VBox>
   </children>
</StackPane>

